{% if projects and projects.length > 0 %}
  <p>Projects collection count (in partial): {{ projects.length }}</p>
  <div>
    {% for project in projects %}
      <article>
        <h2><a href="{{ project.url }}">{{ project.data.title }}</a></h2>
        <p>{{ project.data.metaDescription }}</p>
      </article>
    {% endfor %}
  </div>
{% else %}
  <p>No projects found.</p>
{% endif %}
