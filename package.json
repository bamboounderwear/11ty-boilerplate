{
	"name": "eleventy-basic-setup",
	"version": "1.0.0",
	"description": "A minimal Eleventy setup with partials, layouts, PostCSS, Eleventy Image plugin and Prettier.",
	"scripts": {
	  "start": "eleventy --serve",
	  "build": "eleventy && npm run postcss",
	  "postcss": "postcss src/css/styles.css -o dist/css/styles.css",
	  "prettier": "prettier --write \"**/*.{js,css,njk,json,md}\""
	},
	"dependencies": {
	  "@11ty/eleventy": "^3.0.0"
	},
	"devDependencies": {
	  "autoprefixer": "^10.4.21",
	  "postcss": "^8.5.3",
	  "postcss-cli": "^11.0.1",
	  "prettier": "^3.5.3"
	}
  }