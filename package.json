{
    "name": "eleventy-bootstrap-boilerplate",
    "version": "1.0.0",
    "description": "A minimal Eleventy boilerplate using Bootstrap via SCSS with custom overrides.",
    "main": ".eleventy.js",
    "scripts": {
      "build-css": "sass src/scss/main.scss:src/css/main.css",
      "build": "npm run build-css && eleventy",
      "start": "npm run build-css && eleventy --serve"
    },
    "dependencies": {
      "@11ty/eleventy": "^3.0.0",
      "bootstrap": "^5.3.0",
      "sass": "^1.66.1"
    },
